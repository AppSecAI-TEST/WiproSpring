<?xml version="1.0" encoding="UTF-8"?>
<beans 
xmlns= "http://www.springframework.org/schema/beans"
xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
xmlns:context= "http://www.springframework.org/schema/context"
xsi:schemaLocation= "http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd" >


	<bean id="laptop" class="com.demo.E13_ItemFactory" factory-method="createItemOfType">
		<constructor-arg value="laptop" />
	</bean>
	
	<bean id="mobile" class="com.demo.E13_ItemFactory" factory-method="createItemOfType">
		<constructor-arg value="mobile" />
	</bean>

</beans>